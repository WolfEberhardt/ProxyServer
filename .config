# Hier können Einstellungen vorgenommen werden.
#---------------------------------------------------

auto_setup=True
    # Windows exklusive

host="onlinehost"
    # Standart: localhost
    # localhost = 127.0.0.1
    # onlinehost = Ipadresse deines Gerätes (kann auch manuell eingegeben werden)

port=8888
    # freien Port nach wahl, Standart: 8888
    # Bei Bedarf Firewall überprüfen

logging=True
    # Erstellt eine Log-Datei

colorfull=True
    # Farbiger Output und logging

exclude_domains="www.ignore_this_side.de, "
    # Überschreiben die genutzte Blacklist

max_users=3
    # 0 = nur localhost (bei localhost automatisch 0)
    # Anzahl der maximalen Nutzern des Servers